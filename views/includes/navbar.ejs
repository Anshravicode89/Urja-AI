<style>
  .btn-search {
    background-color: white;
    color: #fff;
    border-radius: 25px;
    padding: 0 1rem 0 1rem;
    border: 2px solid black; /* Add initial border color to make the transition smooth */
    transition: background-color 0.3s, border-color 0.3s; /* Smooth transition for background and border */
}

.btn-search:hover {    
    color: #fff;
    border-color: #fe424d; /* Change the border color to red on hover */
}
.search-inp{
  border-radius: 25px;
  padding: 0.5rem 3rem 0.5rem 3rem;
  border: 2px solid black; /* Add initial border color to make the transition smooth */
  transition: background-color 0.3s, border-color 0.3s; /* Smooth transition for background and border */
  margin-left: 500px;
}
.navbar-nav .search-container {
    margin-left: 20px; /* Adjust this value to move the search bar to the right */
  }
.search-imp:hover{
  border-color: #fe424d;
}
.center-heading {
    position: absolute;
    left: 55%;
    transform: translateX(-50%);
    margin-top: 10px;
    border-right: 5px solid rgb(255, 255, 255);
    width: 18%;
    white-space: nowrap;
    overflow: hidden;
    animation:
      typing 5s steps(60) infinite,
      cursor .4s step-end infinite alternate;

}
/*cursor blinking*/
@keyframes cursor{
  50% { border-color: transparent}
}
/*typing effect*/
@keyframes typing {
    0%, 100% { width: 0; }
    50% { width: 18%; }
  }

/* Electric cursor blink */
@keyframes electricCursor {
  50% {
    border-color: transparent;
  }
}

/* Electric typing effect */
@keyframes electricText {
    0% { width: 0; }
    40% { width: 18%; }
    60%, 100% { width: 18%; }
}

/* Electric glitch effect */
.center-heading h1 {
  font-family: 'Orbitron', sans-serif;
  font-size: 2.5rem;
  position: relative;
  display: inline-block;
  color: #000000; /* Electric green */
  text-shadow: 0 0 10px #00ff00, 0 0 20px #00ff00, 0 0 30px #00ff00;
  animation: electricGlow 2s infinite;
}

@keyframes electricGlow {
  0%, 100% {
    text-shadow: 0 0 10px #00ff00, 0 0 20px #00ff00, 0 0 30px #00ff00;
  }
  50% {
    text-shadow: 0 0 20px #ffffff, 0 0 30px #ffffff;
  }
}



